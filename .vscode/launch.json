    "version": "0.2.0",
    "configurations": [
        {
            "name": "uec_textbooks",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceFolder}/lib/main_staging.dart",
            "args": ["--flavor", "staging"]
        },
        {
            "name": "uec_textbooks (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "${workspaceFolder}/lib/main_staging.dart"
        },
        {
            "name": "uec_textbooks (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "${workspaceFolder}/lib/main_staging.dart"
        }
    ]
}
